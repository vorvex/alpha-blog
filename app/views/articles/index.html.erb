<h1>Listing all articles</h1>

<table style="width:100%">
  <tr>
    <th>ID</th>
    <th>Title</th> 
    <th>Description</th>
    <th>Created</th>
    <th>Updated</th>
    <th>Change</th>
  </tr>
   <% @articles.each do |f| %>
     <tr>
      <td><%= f.id %></td>
      <td><%= f.title %></td> 
      <td><%= f.description %></td>
      <td><%= f.created_at %></td>
      <td><%= f.updated_at %></td>
      <td><%= link_to 'Edit', edit_article_path(f) %> <%= link_to 'Show', article_path(f) %> </td> 
    </tr>
  <% end %>
</table>
<p><%= link_to "Create new Article", new_article_path %></p>
